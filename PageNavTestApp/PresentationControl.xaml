<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PageNavTestApp.PresentationControl"
             Title="PresentationControl">
    <StackLayout>
        <BoxView BackgroundColor="Gray" HeightRequest="10" WidthRequest="600"/>
        <Label Text="DotNet Maui" TextColor="DodgerBlue"
               FontAttributes="Bold" FontSize="Title"
               HorizontalOptions="Center"/>
        <Ellipse Fill="DarkRed" Stroke="Green"
                 StrokeThickness="12"
                 HorizontalOptions="Center"
                 WidthRequest="250"/>
    </StackLayout>
</ContentPage>